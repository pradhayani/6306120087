package faktorial;
import java.util.Scanner;

public class Faktorial {

  
    public static void main(String[] args) {
    
        System.out.print("Input : ");
        Scanner angka = new Scanner(System.in);
        int a= angka.nextInt();
        int total=1;
        for(int i=1;i<=a;i++){
            total=total*i;
        }
        System.out.println("Output : "+total);
    }
    
}
